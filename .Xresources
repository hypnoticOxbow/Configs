!Urxvt
 URxvt*buffered: true
 URxvt*cursorBlink: true
 URxvt*underlineColor: yellow
 URxvt*font:  xft:FuraMono Nerd Font Mono:style=Regular:size=12:antialias=true, xft:octicons:style=Medium:size=12
 URxvt*letterSpace: 0
 URxvt*depth: 32
 URxvt*borderless: 0
 URxvt*internalBorder: 0
 URxvt*externalBorder: 0
 URxvt*borderColor: #1c1b1a
 URxvt*scrollBar: false
 URxvt*loginShell: false
 Urxvt*secondaryScroll:  true    # Enable Shift-PageUp/Down in screen
 URxvt*saveLines: 5000
 URxvt*termName: rxvt-unicode
 URxvt.perl-ext-common: default,matcher
 URxvt.urlLauncher: /usr/bin/vivaldi-stable
 URxvt.matcher.button: 1
 URxvt.perl-ext: default,url-select
 URxvt.perl-ext-common: default,selection-to-clipboard,pasta,matcher,keyboard-select
 URxvt.keysym.M-u: perl:url-select:select_next
 URxvt.url-select.launcher: /usr/bin/firefox
 URxvt.url-select.underline: true
URxvt.underlineURLs:  True

!-*- Perl extensions -*-
URxvt.url-launcher:   /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.keysym.M-Escape:perl:keyboard-select:activate
URxvt.keysym.Control-Shift-V:     perl:pasta:paste
URxvt.keysym.Control-Shift-C:     perl:pasta:copy
URxvt.clipboard.autocopy: true


